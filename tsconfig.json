{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictFunctionTypes": true,
    "strict": true,
    "outDir": "dist",
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist", "spec"]
}